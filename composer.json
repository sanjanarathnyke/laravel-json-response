{
    "name": "json-box/laravel-api-response",
    "description": "A simple and elegant API response formatter for Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "response",
        "formatter",
        "json"
    ],
    "authors": [
        {
            "name": "Sanjana R",
            "email": "sanjanarathnayake@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "JsonBox\\LaravelApiResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonBox\\LaravelApiResponse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JsonBox\\LaravelApiResponse\\ApiResponseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
